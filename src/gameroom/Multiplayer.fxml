<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" stylesheets="@Multiplayer.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gameroom.MultiplayerController">
   <children>
      <TabPane prefHeight="500.0" prefWidth="600.0" stylesheets="@Multiplayer.css" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Game Select">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="500.0" prefWidth="600.0">
                     <children>
                        <Button layoutX="39.0" layoutY="255.0" mnemonicParsing="false" onAction="#openGameThree" prefHeight="160.0" prefWidth="221.0">
                           <graphic>
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../media/blackhole.PNG" />
                                 </image></ImageView>
                           </graphic>
                        </Button>
                        <Button layoutX="336.0" layoutY="255.0" mnemonicParsing="false" onAction="#openGameFour">
                           <graphic>
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../media/Brick%20Break.PNG" />
                                 </image></ImageView>
                           </graphic>
                        </Button>
                        <Button layoutX="191.0" layoutY="35.0" mnemonicParsing="false" onAction="#openGameTwo">
                           <graphic>
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../media/matcher.PNG" />
                                 </image></ImageView>
                           </graphic>
                        </Button>
                        <ComboBox layoutX="337.0" layoutY="415.0" prefHeight="31.0" prefWidth="220.0" promptText="Active Players" />
                        <ComboBox layoutX="39.0" layoutY="415.0" prefHeight="31.0" prefWidth="221.0" promptText="Active Players" />
                        <ComboBox layoutX="190.0" layoutY="195.0" prefHeight="31.0" prefWidth="221.0" promptText="Active Players" />
                        <Label layoutX="191.0" layoutY="14.0" text="Matcher:" />
                        <Label layoutX="39.0" layoutY="234.0" text="Black Hole:" />
                        <Label layoutX="337.0" layoutY="234.0" text="Brick Break:" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="My Profile">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="nameLabel" layoutX="26.0" layoutY="32.0" text="name(change in code)" />
                        <Label fx:id="ageLabel" layoutX="26.0" layoutY="77.0" text="age(change in code)" />
                        <TextArea layoutX="26.0" layoutY="131.0" prefHeight="305.0" prefWidth="219.0" />
                        <ImageView fitHeight="180.0" fitWidth="242.0" layoutX="309.0" layoutY="32.0" pickOnBounds="true" preserveRatio="true" />
                        <Label layoutX="328.0" layoutY="210.0" text="Gamescore: " />
                        <Label layoutX="326.0" layoutY="284.0" text="Wins/Losses:" />
                        <ComboBox layoutX="326.0" layoutY="315.0" prefHeight="31.0" prefWidth="201.0" promptText="Wins/Losses by Game" />
                        <Label layoutX="26.0" layoutY="112.0" text="Bio:" />
                        <Label layoutX="430.0" layoutY="210.0" text="add score" />
                        <Label layoutX="430.0" layoutY="284.0" text="add win/loss" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab text="Online Players">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="20.0" layoutY="20.0" text="Online Players:">
                           <font>
                              <Font size="48.0" />
                           </font>
                        </Label>
                        <ListView layoutX="20.0" layoutY="90.0" prefHeight="315.0" prefWidth="560.0" />
                        <Button layoutX="482.0" layoutY="416.0" mnemonicParsing="false" text="View Profile" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
